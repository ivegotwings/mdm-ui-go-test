    "scripts": {
        "start": "npm run dev",
        "deps": "npm install",
        "dev": "npm run copy-polymer-overrides & gulp generate-index & gulp app-nodemon --unbundled",
        "serve": "polymer serve --entrypoint index.html --npm",
        "dataaccess": "npm uninstall ui-platform-dataaccess --no-save && npm install",
        "test": "polymer test --npm --module-resolution=node",
        "test-local": "polymer test --npm --module-resolution=node -p",
        "webpack-analyze": "webpack --env production --analyze",
        "lint/all": "node node_modules/eslint/bin/eslint --fix 'src/**' || true",
        "wca": "npx web-component-analyzer 'node_modules/ui-platform-elements/src/**/*' 'src/**/*' --format json --discoverLibraryFiles --outFile ./.storybook/custom-elements.json",
        "compile-storybook-deps": "npm run wca && gulp compile-storybook-deps",
        "build-storybook": "npm run compile-storybook-deps && build-storybook -o build/ui-platform/static/bundled/docs/sdk/main/storybook --debug-webpack",
        "storybook": "npm run compile-storybook-deps && start-storybook -p 6007 --debug-webpack",
        "copy-polymer-overrides": "gulp copy-polymer-overrides --max_old_space_size=4096",
        "build-step01-npm-install": "npm install",
        "build-step02-eslint": "gulp eslint-src --max_old_space_size=4096",
        "build-step03-copy-polymer-overrides": "gulp copy-polymer-overrides --max_old_space_size=4096",
        "build-step04-clean-dest": "gulp prod-delete --max_old_space_size=4096",
        "build-step05-unbundled-generate-index": "gulp generate-index --production --plugin",
        "build-step06-unbundled-create-transpiled-src": "gulp babel-transpile",
        "build-step07-unbundled-compile-polymer-build": "NODE_OPTIONS=--max_old_space_size=8192 polymer build --module-resolution=node",
        "build-step08-unbundled-copy-plugins": "gulp move-unbundled-build --production",
        "build-step09-prod-compile-server": "npm run build-storybook && gulp build-server --max_old_space_size=4096",
        "build-step10-prod-generate-index": "gulp generate-index --production",
        "build-step11-prod-truncate-runtimeindex": "gulp truncate-runtimeindex",
        "build-step12-prod-compile-webpack-build": "webpack --env production",
        "build-step13-wrap-up": "gulp build-wrap-up --production --max_old_space_size=4096",
        "local-build-step01-npm-install": "npm install",
        "local-build-step02-eslint": "gulp eslint-src --max_old_space_size=4096",
        "local-build-step03-copy-polymer-overrides": "gulp copy-polymer-overrides --max_old_space_size=4096",
        "local-build-step04-clean-dest": "gulp prod-delete --max_old_space_size=4096",
        "local-build-step05-unbundled-generate-index": "gulp generate-index --production --plugin",
        "local-build-step06-unbundled-create-transpiled-src": "gulp babel-transpile",
        "local-build-step07-unbundled-compile-polymer-build": "NODE_OPTIONS=--max_old_space_size=8192 polymer build --module-resolution=node",
        "local-build-step08-unbundled-copy-plugins": "gulp move-unbundled-build --development",
        "local-build-step09-prod-compile-server": "npm run build-storybook && gulp build-server --max_old_space_size=4096",
        "local-build-step10-prod-generate-index": "gulp generate-index --production",
        "local-build-step11-prod-truncate-runtimeindex": "gulp truncate-runtimeindex",
        "local-build-step12-prod-compile-webpack-build": "webpack --env development",
        "local-build-step13-wrap-up": "gulp build-wrap-up --production --max_old_space_size=4096",
        "noplugin-dev": "gulp generate-index --production && gulp truncate-runtimeindex && webpack --env production && gulp build-server --max_old_space_size=4096 && gulp build-wrap-up --production --max_old_space_size=4096",
        "eslint": "gulp eslint-src --max_old_space_size=4096"
    },
